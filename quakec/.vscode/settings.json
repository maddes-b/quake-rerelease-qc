{
    "files.associations": {
        "*.qc": "quakec",
        "progs.src": "quakec"
    },
    "files.encoding": "utf8",
    "[quakec]": {
        "files.encoding": "windows1252"
    },
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "files.trimTrailingWhitespace": true,
    "[markdown]": {
        "files.trimTrailingWhitespace": false
    }
}